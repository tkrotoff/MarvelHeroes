{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "description": "Marvel Heroes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build coverage stats.json",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "webpack-dev-server -d --host 0.0.0.0",
    "start:prod": "webpack-dev-server -p --compress --host 0.0.0.0",
    "build": "webpack -d",
    "build:watch": "webpack -d --watch",
    "build:prod": "webpack -p",
    "build:prod:analyze": "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "jest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "NODE_ENV=test jest --config jest-e2e.config.js",
    "tsc": "tsc",
    "format": "prettier --write '**/*'",
    "lint": "npm run tsc && eslint . '**/*.{js,ts,tsx}' && stylelint '**/*.scss'",
    "precommit": "lint-staged --verbose",
    "prepush": "npm run test && npm run test:e2e",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@sentry/browser": "^6.0.3",
    "blueimp-md5": "^2.18.0",
    "bootstrap": "^4.6.0",
    "core-js": "^3.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^4.0.1",
    "@types/blueimp-md5": "^2.7.0",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "^26.0.20",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/postcss-preset-env": "^6.7.1",
    "@types/puppeteer": "^5.4.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.0",
    "@types/sass": "^1.16.0",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "html-webpack-plugin": "^4.5.1",
    "html-webpack-tags-plugin": "^2.0.17",
    "http-server": "^0.12.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.5",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0",
    "purgecss-webpack-plugin": "^4.0.0",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "stylelint-prettier": "^1.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}
