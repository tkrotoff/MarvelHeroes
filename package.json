{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "description": "Marvel Heroes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build coverage stats.json",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "webpack-dev-server -d --host 0.0.0.0",
    "start:prod": "webpack-dev-server -p --compress --host 0.0.0.0",
    "build": "webpack -d",
    "build:watch": "webpack -d --watch",
    "build:prod": "webpack -p",
    "build:prod:analyze": "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "test": "NODE_ENV=test jest --verbose",
    "test:coverage": "jest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "NODE_ENV=test jest --config jest-e2e.config.js",
    "tsc": "tsc",
    "format": "prettier --write '**/*'",
    "lint": "npm run tsc && eslint . '**/*.{js,ts,tsx}' && stylelint '**/*.scss'",
    "prepare": "husky install",
    "precommit": "lint-staged --verbose",
    "prepush": "npm run test && npm run test:e2e",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@sentry/browser": "^6.15.0",
    "blueimp-md5": "^2.19.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.19.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/blueimp-md5": "^2.18.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.0.3",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/puppeteer": "^5.4.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@types/sass": "^1.43.1",
    "@types/webpack-dev-server": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.4.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "eslint-plugin-unicorn": "^39.0.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "http-server": "^14.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-puppeteer": "^6.0.2",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "prettier": "^2.5.1",
    "puppeteer": "^12.0.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-twbs-bootstrap": "^2.2.4",
    "stylelint-prettier": "^1.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
