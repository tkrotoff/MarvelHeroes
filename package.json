{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "description": "Marvel Heroes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build coverage stats.json",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "webpack serve --mode=development --host 0.0.0.0",
    "start:prod": "webpack serve --mode=production --compress --host 0.0.0.0",
    "build": "node --trace-deprecation node_modules/.bin/webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build:prod:analyze": "webpack --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "tsc": "tsc",
    "format": "prettier --write '**/*'",
    "lint": "npm run tsc && eslint . '**/*.{js,ts,tsx}' && stylelint '**/*.scss'",
    "prepare": "husky install",
    "precommit": "lint-staged --verbose",
    "prepush": "npm run test && npm run test:e2e",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "blueimp-md5": "^2.19.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.22.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@playwright/test": "^1.21.1",
    "@testing-library/react": "^13.1.1",
    "@types/blueimp-md5": "^2.18.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/mini-css-extract-plugin": "^2.5.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webpack-dev-server": "^4.7.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-playwright": "^0.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "husky": "^7.0.4",
    "jest": "^28.0.2",
    "jest-environment-jsdom": "^28.0.2",
    "lint-staged": "^12.4.1",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.4",
    "prettier": "^2.6.2",
    "purgecss-webpack-plugin": "^4.1.3",
    "react-test-renderer": "^18.1.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-twbs-bootstrap": "^3.0.1",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
