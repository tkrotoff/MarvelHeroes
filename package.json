{
  "name": "marvel-heroes",
  "version": "0.1.0",
  "description": "Marvel Heroes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build coverage stats.json",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "start": "webpack serve --mode=development --host 0.0.0.0",
    "start:prod": "webpack serve --mode=production --compress --host 0.0.0.0",
    "build": "node --trace-deprecation node_modules/.bin/webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "build:prod:analyze": "webpack --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json build",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "tsc": "tsc",
    "format": "prettier --write '**/*'",
    "lint": "npm run tsc && eslint . '**/*.{js,ts,tsx}' && stylelint '**/*.scss'",
    "prepare": "husky install",
    "precommit": "lint-staged --verbose",
    "prepush": "npm run test && npm run test:e2e",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install",
    "count-loc:js": "git ls-files | grep --extended-regexp '\\.(jsx?|tsx?)$' | xargs wc -l",
    "count-loc:js:src": "git ls-files | grep --invert-match --extended-regexp '\\.test(\\.e2e)?\\.(jsx?|tsx?)$' | grep --invert-match '__mocks__' | grep --extended-regexp 'src/.*\\.(jsx?|tsx?)$' | xargs wc -l",
    "count-loc:js:tests": "git ls-files | grep --extended-regexp '\\.test(\\.e2e)?\\.(jsx?|tsx?)$' | xargs wc -l",
    "count-loc:css": "git ls-files | grep --extended-regexp '\\.s?css$' | xargs wc -l"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.6",
    "@sentry/react": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "blueimp-md5": "^2.19.0",
    "bootstrap": "^5.2.3",
    "core-js": "^3.26.1",
    "event-target-polyfill": "^0.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.4",
    "throw-on": "^0.7.0",
    "whatwg-fetch": "^3.6.2",
    "yet-another-abortcontroller-polyfill": "^0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.28.1",
    "@testing-library/react": "^13.4.0",
    "@types/blueimp-md5": "^2.18.0",
    "@types/glob": "^8.0.0",
    "@types/jest": "^29.2.4",
    "@types/react-dom": "^18.0.9",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-playwright": "^0.11.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unicorn": "^45.0.1",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.1",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-twbs-bootstrap": "^7.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  }
}
